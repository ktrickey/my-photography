{% extends "layouts/base.njk" %} {% block content %}
  <!-- Home Section -->
  <section class="page-section bg-dark light-content" id="home">
    <div class="container relative text-center">

      <div class="row">
        <div class="col-lg-10 offset-lg-1">
          <h1 class="hs-line-4 mb-30 mb-xs-20 wow fadeInUpShort" data-wow-delay=".1s">
            <span class="d-inline-block bg-gray-dark round px-3 pb-1">{{ category.title }}</span>
          </h1>
        </div>
      </div>

    </div>
  </section>
  <!-- End Home Section -->
<div class="container relative">

  <!-- Photo Grid -->
  <div class="row mb-30 mb-xs-10">

  {%- for gal in category.galleries %}
    <div class="col-md-4">

      <div class="post-prev-img mb-30 wow fadeScaleIn" data-wow-duration="1s">
        <a href="images/portfolio/full-project-1.jpg" class="lightbox-gallery-2 mfp-image">
          <img src="images/portfolio/projects-1.jpg" alt="" /></a>
        {{ gal }}
      </div>

    </div>

    {% endfor %}
  </div>
</div>
{% endblock %}
